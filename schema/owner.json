{
    "$schema": "http://json-schema.org/draft-07/schema",
    "properties": {
        "id":{
            "type":"integer"
        },
        "name":{
                "type":"string"
        },
        "address":{
            "type":"string"
        },
        "phone":{
            "type":"string",
            "pattern": "^(\\([0-9]{3}\\))?[0-9]{3}-[0-9]{4}$"
        },
        "email":{
            "type":"string",
            "format": "email"
        }
    },
    "required": ["name","phone","email","address"]
}